{
    "name": "wp-graph-view",
    "version": "2.0.0",
    "description": "A React-based WordPress plugin for Cytoscape.js graph visualization.",
    "scripts": {
        "build": "NODE_ENV=production webpack",
        "dev": "webpack --watch",
        "bundle": "pnpm run build && zip -r wp-graph-view.zip build/* wp-graph-view.php includes/* plugin.php"
    },
    "dependencies": {
        "@wordpress/components": "^25.13.0",
        "@wordpress/data": "^9.17.0",
        "@wordpress/edit-post": "^7.24.0",
        "@wordpress/element": "^5.24.0",
        "@wordpress/i18n": "^4.47.0",
        "@wordpress/plugins": "^6.15.0",
        "cytoscape": "^3.30.4",
        "cytoscape-cola": "^2.5.1",
        "react": "^18.2.0",
        "react-cytoscapejs": "^2.0.0",
        "react-dom": "^18.2.0",
        "react-select": "^5.9.0",
        "@wordpress/icons": "^9.42.0",
        "@wordpress/primitives": "^3.41.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/preset-react": "^7.18.6",
        "babel-loader": "^9.1.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    }
}